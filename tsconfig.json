{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "esModuleInterop": true,
    "baseUrl": "src",
    "paths": {
      "@/*": [
        "components/*"
      ],
      "@images/*": [
        "images/*"
      ],
      "@storybook": [
        "components/storybook/index"
      ]
    },
    "typeRoots": [
      "node_modules/@types"
    ],
    "jsx": "react",
    "target": "es5",
    "allowJs": true
  }
}
